{
  "name": "privateai-electron",
  "version": "0.1.0",
  "description": "Private AI desktop app using Electron and node-llama-cpp",
  "main": "build/main.js",
  "type": "commonjs",
  "scripts": {
    "build:webpack": "webpack --config webpack.main.config.cjs && webpack --config webpack.renderer.config.cjs",
    "dev": "npm run build:webpack && cross-env NODE_ENV=development electron .",
    "build": "npm run build:webpack && electron-builder",
    "package": "npm run build",
    "start": "electron ."
  },
  "author": "CaribbeanTechS",
  "license": "UNLICENSED",
  "dependencies": {
    "node-llama-cpp": "^3.14.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^9.0.3",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "path-browserify": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^31.3.0",
    "electron-builder": "^24.13.3",
    "html-webpack-plugin": "^5.6.4",
    "style-loader": "^4.0.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "build": {
    "appId": "com.privateai.desktop",
    "asar": false,
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "resources/models",
        "to": "models"
      },
      {
        "from": "resources/backend",
        "to": "backend"
      }
    ],
    "mac": {
      "target": "dmg"
    }
  }
}
